URxvt*scrollBar: false
URxvt*transparent: false
URxvt*shading: false
URxvt*blurRadius: false
URxvt.jumpScroll: true

URxvt.font: xft:iosevka-term:size=10
! URxvt.font: xft:Droid Sans Mono for Powerline:size=9
URxvt.letterSpace: -1
URxvt.internalBorder: 3

! Launch urls from urxvt
urxvt*perl-lib:         /usr/lib/urxvt/perl/
urxvt*perl-ext-common:  default,matcher
urxvt*matcher.button:   1
urxvt*urlLauncher:      /usr/bin/qutebrowser

URxvt.visualBell: false
URxvt.bell-command: false
URxvt.mapAlert: false
URxvt.scrollTtyOutput: false
URxvt.backgroundPixmap: /home/vidr/Development/graphic/Logos/arivarton/squarelogo.png

